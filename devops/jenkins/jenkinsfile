pipeline {
    agent any
    tools {
        maven "M3"
    }
    stages {
        stage('build') {
       	   agent any
           steps {
                git 'https://github.com/vrairaauf/green-phone-numbers-server.git'
                
                 bat "mvn -Dmaven.test.failure.ignore=true clean package"
            }
        }
        stage('deploy') {
         	agent any
         	steps {
	             echo "deploy success"
	         }  
        }
    }
}